  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.14.0",
    "egg-ci": "^1.13.1",
    "egg-mock": "^3.24.1",
    "eslint": "^6.8.0",
    "eslint-config-egg": "^7.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "factory-girl": "^5.0.4",
    "jsonwebtoken": "^8.5.1",
    "sequelize-cli": "^5.5.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-oneframephoto",
    "stop": "egg-scripts stop --title=egg-server-oneframephoto",
    "dev": "export NODE_ENV='development' && egg-bin dev",
    "dev:win": "set NODE_ENV='development' && egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "migrate:new": "npx sequelize migration:create --name init",
    "migrate:up": "npx sequelize db:migrate",
    "migrate:up:prod": "export NODE_ENV='production' && npx sequelize db:migrate",
    "migrate:down": "npx sequelize db:migrate:undo",
    "seed": "npx sequelize db:seed:all",
    "seed:prod": "export NODE_ENV='production' && npx sequelize db:seed:all",
    "local": "export NODE_ENV='development' && EGG_SERVER_ENV=local && egg-bin dev",
    "prod": "export NODE_ENV='production' && EGG_SERVER_ENV=prod && egg-bin dev",
    "zip": "tar -zcvf ./release.tgz .",
    "unzip": "tar zxvf release.tgz -C ./",
    "deploy:prod": "npm run migrate:up:prod && npm run seed:prod && npm run start"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
