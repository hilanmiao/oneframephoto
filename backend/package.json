    "egg-oss": "^2.0.0",
    "egg-passport": "^2.1.0",
    "egg-passport-asicz-weixin": "^1.0.7",
    "egg-passport-dingtalk": "^1.0.1",
    "egg-passport-github": "^1.0.0",
    "egg-passport-local": "^1.2.1",
    "egg-redis": "^2.4.0",
    "egg-router-plus": "^1.3.1",
    "egg-scripts": "^2.11.1",
    "egg-sequelize": "^5.2.0",
    "egg-validate": "^2.0.2",
